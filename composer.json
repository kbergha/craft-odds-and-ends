{
    "name": "supercool/tools",
    "description": "A collection of useful things for Craft websites",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "supercooltools"
    ],
    "support": {
        "docs": "https://github.com/supercool/tools/blob/master/README.md",
        "issues": "https://github.com/supercool/tools/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Supercool",
            "homepage": "http://supercooldesign.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "supercool\\tools\\": "src/"
        }
    },
    "extra": {
        "name": "Tools",
        "handle": "tools",
        "schemaVersion": "1.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/supercool/tools/master/CHANGELOG.md",
        "class": "supercool\\tools\\Tools"
    }
}
