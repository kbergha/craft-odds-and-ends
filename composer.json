{
    "name": "spicyweb/tools",
    "description": "A collection of useful things for Craft websites",
    "type": "craft-plugin",
    "version": "2.2.3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin"
    ],
    "support": {
        "docs": "https://github.com/spicywebau/tools/blob/master/README.md",
        "issues": "https://github.com/spicywebau/tools/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Spicy Web",
            "homepage": "https://spicyweb.com.au"
        }
    ],
    "require": {
        "craftcms/cms": "^3.7.46"
    },
    "autoload": {
        "psr-4": {
          "spicyweb\\tools\\": "src/"
        }
    },
    "extra": {
        "name": "Tools",
        "handle": "tools",
        "schemaVersion": "3.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/spicywebau/tools/master/CHANGELOG.md",
        "class": "spicyweb\\tools\\Tools"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/ecs": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix"
    }
}
